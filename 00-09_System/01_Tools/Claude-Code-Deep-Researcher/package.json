{
  "name": "claude-code-deep-researcher",
  "version": "1.0.0",
  "description": "Claude Agent SDK JSONL Parser - Comprehensive conversation analyzer built with Claude Agent SDK",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "claude-researcher": "./dist/cli.js",
    "conversation-analyzer": "./dist/conversation-analyzer.js",
    "context-optimizer": "./dist/cli/context-optimizer-cli.js",
    "validate-session-resume": "./dist/cli/validate-session-resume.js"
  },
  "publishConfig": {
    "bin": {
      "claude-researcher": "./dist/cli.js",
      "conversation-analyzer": "./dist/conversation-analyzer.js",
      "context-optimizer": "./dist/cli/context-optimizer-cli.js",
      "validate-session-resume": "./dist/cli/validate-session-resume.js"
    }
  },
  "preferGlobal": true,
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun && bun build src/cli.ts --outdir dist --target bun && bun build src/cli/conversation-analyzer.ts --outdir dist --target bun --outfile conversation-analyzer.js && bun build src/cli/context-optimizer-cli.ts --outdir dist/cli --target bun && bun build src/cli/validate-session-resume.ts --outdir dist/cli --target bun",
    "start": "bun run dist/index.js",
    "typecheck": "bun run tsc --noEmit",
    "test": "bun test",
    "analyze": "bun run src/cli/conversation-analyzer.ts",
    "conversation:analyze": "conversation-analyzer analyze",
    "conversation:list": "conversation-analyzer list",
    "conversation:steps": "conversation-analyzer steps",
    "conversation:export": "conversation-analyzer export",
    "conversation:validate": "conversation-analyzer validate",
    "dev:web": "bun --hot run src/web/server.ts",
    "start:web": "bun run src/web/server.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.67",
    "@anthropic-ai/sdk": "^0.69.0",
    "@anthropic-ai/tokenizer": "^0.0.4",
    "commander": "^14.0.1",
    "glob": "^11.0.3",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bun": "^1.3.4",
    "@types/uuid": "^10.0.0",
    "playwright": "^1.57.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "claude",
    "claude-agent",
    "claude-agent-sdk",
    "jsonl",
    "parser",
    "conversation",
    "analyzer",
    "typescript",
    "bun",
    "cli",
    "analysis",
    "validation"
  ],
  "author": "Claude SDK App",
  "license": "MIT",
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "peerDependenciesMeta": {
    "@google/clasp": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  }
}
